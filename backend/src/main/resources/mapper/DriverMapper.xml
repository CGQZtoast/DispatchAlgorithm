<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zard.backend.dao.DriverMapper">
    <!--查找商品-->
    <select id="getDriverMarkers" resultType="com.zard.backend.model.DriverMarkerModel">
        select id, longitude, latitude
        from driver
        where state = true
    </select>
</mapper>